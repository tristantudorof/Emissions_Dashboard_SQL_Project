{
  "datasets": [
    {
      "name": "f2283127",
      "displayName": "Location Data",
      "queryLines": [
        "SELECT latitude,\n",
        "longitude, \n",
        "`GHG emissions mtons CO2e` as Emissions\n",
        "\n",
        "From emissions.default.emissions_data"
      ]
    },
    {
      "name": "01cf1bf3",
      "displayName": "Emissions Per Person",
      "queryLines": [
        "SELECT \n",
        "    county_state_name,\n",
        "    population,\n",
        "    CAST(\n",
        "        REPLACE(`GHG emissions mtons CO2e`, ',', '') \n",
        "        AS DOUBLE\n",
        "    ) \n",
        "    / NULLIF(\n",
        "        CAST(\n",
        "            REPLACE(population, ',', '') \n",
        "            AS DOUBLE\n",
        "        ), \n",
        "        0\n",
        "    ) AS Emissions_per_person\n",
        "FROM emissions_data\n",
        "ORDER BY Emissions_per_person DESC"
      ],
      "catalog": "emissions",
      "schema": "default"
    },
    {
      "name": "59301632",
      "displayName": "Total Emissions Per State",
      "queryLines": [
        "SELECT \n",
        "    state_abbr,\n",
        "    SUM(CAST(\n",
        "        REPLACE(`GHG emissions mtons CO2e`, ',', '') \n",
        "        AS DOUBLE\n",
        "     ) ) AS Total_Emissions\n",
        "FROM emissions_data\n",
        "GROUP BY state_abbr\n",
        "ORDER BY Total_Emissions DESC\n",
        "LIMIT 10"
      ],
      "catalog": "emissions",
      "schema": "default"
    },
    {
      "name": "2ca3181b",
      "displayName": "County Emissions",
      "queryLines": [
        "SELECT \n",
        "    county_state_name,\n",
        "    population,\n",
        "    CAST(\n",
        "        REPLACE(`GHG emissions mtons CO2e`, ',', '') \n",
        "        AS DOUBLE) as Total_Emissions\n",
        "FROM emissions_data\n",
        "ORDER BY Total_Emissions DESC\n",
        "LIMIT 10"
      ],
      "catalog": "emissions",
      "schema": "default"
    }
  ],
